{
    "name": "gusty",
    "version": "1.0.2-beta.03",
    "description": "Styled Component approach to writing Tailwind variant scoped classes with a nested SCSS/GraphQL like syntax",
    "scripts": {
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ryanlegler/gusty.git"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/gusty.umd.js",
    "module": "./dist/gusty.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/gusty.es.js",
            "require": "./dist/gusty.umd.js"
        }
    },
    "keywords": [],
    "author": "Ryan Legler",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ryanlegler/gusty/issues"
    },
    "homepage": "https://github.com/ryanlegler/gusty#readme",
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/builder-vite": "^0.3.0",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "0.0.13",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.2",
        "@types/babel__core": "^7.20.0",
        "@types/babel__template": "^7.4.1",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.9.0",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "@vitest/ui": "^0.27.2",
        "babel-loader": "^8.3.0",
        "concurrently": "^7.6.0",
        "esbuild": "^0.14.11",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jsdom": "^21.0.0",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-dts": "^1.7.1",
        "vitest": "^0.27.1"
    },
    "dependencies": {
        "tailwind-merge": "^1.3.0"
    }
}
